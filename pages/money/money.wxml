<view class="money">
<form bindsubmit="withdrawals">
  <view>
    <text>我的余额</text>
    <text class="wbalance"><text class="balance">{{money}}</text>元</text>
  </view>
  <view class="txje"><text>提取数量</text></view>
  <view class="deposit">
    <text class="icon">￥</text>
    <input bindinput="textinput" data-type="1" type="digit" maxlength='8'></input>
  </view>
  <view class="tips">
    <text>提取到微信钱包，</text>
    <text>全部提取</text>
  </view>
  <button formType="submit" type="primary" class="btncash">确认提取</button>
  <view class="tips center">1-5个工作日到帐</view>
</form>
</view>